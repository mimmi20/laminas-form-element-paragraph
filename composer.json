{
  "name": "mimmi20/laminas-form-element-paragraph",
  "description": "Provides a GitHub repository laminas-form-element-paragraph for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-paragraph",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-paragraph/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-paragraph"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "laminas/laminas-form": "^3.8.0",
    "laminas/laminas-inputfilter": "^2.22.1",
    "laminas/laminas-view": "^2.25.2"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "laminas/laminas-i18n": "^2.21.0",
    "laminas/laminas-modulemanager": "^2.14.0",
    "mimmi20/coding-standard": "^3.0.23",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.9.4",
    "phpstan/phpstan-deprecation-rules": "^1.1.1",
    "phpstan/phpstan-phpunit": "^1.3.2",
    "phpunit/phpunit": "^9.5.27"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Paragraph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Paragraph\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Paragraph",
      "config-provider": "Mimmi20\\Form\\Paragraph\\ConfigProvider"
    }
  }
}
