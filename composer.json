{
  "name": "mimmi20/laminas-form-element-paragraph",
  "description": "Provides a GitHub repository laminas-form-element-paragraph for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-paragraph",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-paragraph/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-paragraph"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-form": "^3.21.0",
    "laminas/laminas-inputfilter": "^2.31.0",
    "laminas/laminas-view": "^2.36.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.8",
    "laminas/laminas-i18n": "^2.29.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "mimmi20/coding-standard": "^6.0.1",
    "nikic/php-parser": "^5.3.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.9",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^11.5.1",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^1.2.10",
    "rector/type-perfect": "^1.0.0",
    "symfony/process": "^7.2.0",
    "symplify/phpstan-rules": "^13.0.1",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^1.0.0",
    "tomasvotruba/unused-public": "^1.1.0"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Paragraph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Paragraph\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Paragraph",
      "config-provider": "Mimmi20\\Form\\Paragraph\\ConfigProvider"
    }
  }
}
