{
  "name": "mimmi20/laminas-form-element-paragraph",
  "type": "library",
  "description": "Provides a GitHub repository laminas-form-element-paragraph for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/mimmi20/laminas-form-element-paragraph",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-form": "^3.0.1",
    "laminas/laminas-inputfilter": "^2.12.0"
  },
  "require-dev": {
    "laminas/laminas-dependency-plugin": "^2.2.0",
    "laminas/laminas-modulemanager": "^2.11.0",
    "mimmi20/coding-standard": "^2.5.0",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Element\\Paragraph",
      "config-provider": "Mimmi20\\Form\\Element\\Paragraph\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Element\\Paragraph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Element\\Paragraph\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-paragraph/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-paragraph"
  }
}
